  supports_gold_linker: false
  supports_incremental_linker: false
  supports_fission: false
  supports_interface_shared_objects: false
  supports_normalizing_ar: false
  supports_start_end_lib: false
  supports_thin_archives: false
  target_libc: "roborio"
  target_cpu: "roborio"
  target_system_name: "roborio"
  toolchain_identifier: "roborio_linux"

  tool_path { name: "ar" path: "/usr/bin/arm-frc-linux-gnueabi-ar" }
  tool_path { name: "compat-ld" path: "/usr/bin/arm-frc-linux-gnueabi-ld" }
  tool_path { name: "cpp" path: "/usr/bin/arm-frc-linux-gnueabi-cpp" }
  tool_path { name: "dwp" path: "/usr/bin/arm-frc-linux-gnueabi-dwp" }
  tool_path { name: "gcc" path: "/usr/bin/arm-frc-linux-gnueabi-gcc" }
  tool_path { name: "gcov" path: "/usr/bin/arm-frc-linux-gnueabi-gcov" }

  # tool_path { name: "ar" path: "roborio-compiler/arm-frc-linux-gnueabi-ar"}
  # tool_path { name: "compat-ld" path: "roborio-compiler/arm-frc-linux-gnueabi-ld" }
  # tool_path { name: "cpp" path: "roborio-compiler/arm-frc-linux-gnueabi-cpp" }
  # tool_path { name: "dwp" path: "roborio-compiler/arm-frc-linux-gnueabi-dwp" }
  # tool_path { name: "gcc" path: "roborio-compiler/arm-frc-linux-gnueabi-gcc" }
  # tool_path { name: "gcov" path: "roborio-compiler/arm-frc-linux-gnueabi-gcov" }
  cxx_flag: "-std=c++1y"
  linker_flag: "-lstdc++"

  tool_path { name: "ld" path: "/usr/bin/arm-frc-linux-gnueabi-ld" }
  tool_path { name: "nm" path: "/usr/bin/arm-frc-linux-gnueabi-nm" }
  tool_path { name: "objcopy" path: "/usr/bin/arm-frc-linux-gnueabi-objcopy" }

  # tool_path { name: "ld" path: "roborio-compiler/arm-frc-linux-gnueabi-ld" }
  # tool_path { name: "nm" path: "roborio-compiler/arm-frc-linux-gnueabi-nm" }
  # tool_path { name: "objcopy" path: "roborio-compiler/arm-frc-linux-gnueabi-objcopy" }

  tool_path { name: "objdump" path: "/usr/bin/arm-frc-linux-gnueabi-objdump" }
  tool_path { name: "strip" path: "/usr/bin/arm-frc-linux-gnueabi-strip" }

  # tool_path { name: "objdump" path: "roborio-compiler/arm-frc-linux-gnueabi-objdump" }
  # tool_path { name: "strip" path: "roborio-compiler/arm-frc-linux-gnueabi-strip" }
  linker_flag: "-lm"
}


